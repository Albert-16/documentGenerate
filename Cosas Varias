MERGE INTO EVERTECDTA.GCTDC09L1 T
USING (VALUES
  (CAST(? AS VARCHAR(100)), CAST(? AS DECIMAL(2,0))),
  (CAST(? AS VARCHAR(100)), CAST(? AS DECIMAL(2,0)))
) S (ID, ESTADO)
ON T.GCTDC09001 = S.ID
WHEN MATCHED THEN
  UPDATE SET
    T.GCTDC09014 = S.ESTADO,
    T.GCTDC09007 = CAST(? AS CHAR(8)),
    T.GCTDC09008 = CAST(? AS CHAR(6))
